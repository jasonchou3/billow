import Artisan from '../libs/framework/Artisan'

(async () => {
    try {

        const artisan = new Artisan();
        const app = await artisan.init(__dirname, 'console');
        try {
            app.make('console_kernel');

            await artisan.handle();
        } catch (e) {
            console.log(e)
        }

        await app.destroy();
    } catch (e) {
        console.log(e)
    }

})();